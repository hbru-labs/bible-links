{
  "name": "bible-links",
  "version": "0.0.3",
  "scripts": {
    "tailwind:compile": "pnpm tailwindcss -i ./src/app.css -o ./static/tailwind.css",
    "tailwind:compile:watch": "pnpm tailwindcss -i ./src/app.css -o ./static/tailwind.css --watch",
    "dev": "pnpm tailwind:compile:watch & vite dev --port 3000 --host 0.0.0.0",
    "build": "pnpm tailwind:compile && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "test:ava": "ava",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.20.0",
    "@sveltejs/adapter-node": "1.0.0-next.85",
    "@sveltejs/adapter-vercel": "1.0.0-next.66",
    "@sveltejs/kit": "1.0.0-next.398",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.7",
    "ava": "^4.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "vite": "3.0.4"
  },
  "type": "module",
  "dependencies": {
    "@elastic/elasticsearch": "^8.2.1",
    "@google-cloud/storage": "^6.2.3",
    "@google-cloud/text-to-speech": "^4.0.0",
    "@google-cloud/translate": "^7.0.0",
    "@latitudelabs/urlly": "^0.1.0",
    "@sentry/browser": "^7.7.0",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "@upstash/redis": "^1.3.5",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "googleapis": "^105.0.0",
    "got": "^12.1.0",
    "openai": "^3.0.0"
  },
  "ava": {
    "files": [
      "tests/**/*.ava.ts"
    ],
    "extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
		]
  }
}